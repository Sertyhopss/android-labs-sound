<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <ImageButton
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@android:drawable/ic_menu_delete"
            android:id="@+id/imageButton4"
            tools:ignore="MissingConstraints" app:layout_constraintStart_toEndOf="@+id/imageButton2"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginEnd="31dp"
            android:layout_marginTop="32dp" android:layout_marginBottom="99dp"
            app:layout_constraintBottom_toTopOf="@+id/seekBar2"/>
    <ImageButton
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@android:drawable/ic_menu_call"
            android:id="@+id/imageButton"
            tools:ignore="MissingConstraints" android:layout_marginTop="32dp"
            app:layout_constraintEnd_toStartOf="@+id/imageButton3" android:layout_marginBottom="10dp"
            app:layout_constraintBottom_toTopOf="@+id/imageButton5" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="32dp" android:layout_marginEnd="10dp"/>
    <ImageButton
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@android:drawable/btn_star_big_on"
            android:id="@+id/imageButton2"
            tools:ignore="MissingConstraints" app:layout_constraintStart_toEndOf="@+id/imageButton3"
            app:layout_constraintEnd_toStartOf="@+id/imageButton4" app:layout_constraintTop_toTopOf="@+id/imageButton3"
            app:layout_constraintBottom_toBottomOf="@+id/imageButton3" android:layout_marginEnd="10dp"
    />
    <ImageButton
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@android:drawable/ic_lock_idle_alarm"
            android:id="@+id/imageButton3"
            tools:ignore="MissingConstraints" android:layout_marginTop="32dp"
            app:layout_constraintStart_toEndOf="@+id/imageButton" app:layout_constraintEnd_toStartOf="@+id/imageButton2"
            android:layout_marginBottom="10dp" app:layout_constraintBottom_toTopOf="@+id/imageButton6"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginEnd="8dp"/>
    <ImageButton
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@android:drawable/ic_menu_edit"
            android:id="@+id/imageButton5"
            tools:ignore="MissingConstraints" app:layout_constraintTop_toBottomOf="@+id/imageButton"
            app:layout_constraintEnd_toStartOf="@+id/imageButton6" android:layout_marginBottom="529dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="32dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginEnd="10dp"/>
    <ImageButton
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@android:drawable/ic_dialog_alert"
            android:id="@+id/imageButton6"
            tools:ignore="MissingConstraints" app:layout_constraintTop_toBottomOf="@+id/imageButton3"
            app:layout_constraintStart_toEndOf="@+id/imageButton5" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginBottom="529dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginEnd="32dp"/>
    <SeekBar
            android:layout_width="0dp"
            android:layout_height="0dp" android:id="@+id/seekBar2"
            android:layout_marginTop="259dp"
            android:layout_marginBottom="50dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/switch1" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="28dp" android:layout_marginEnd="28dp"/>
    <Switch
            android:text="loop"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/switch1"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toBottomOf="@+id/seekBar2"
            android:layout_marginEnd="36dp" android:layout_marginBottom="352dp"
            app:layout_constraintEnd_toEndOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>